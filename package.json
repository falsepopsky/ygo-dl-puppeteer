{
  "name": "ygodl-box-scrapper",
  "version": "0.0.1",
  "description": "Yu-Gi-Oh! DUEL LINKS box scrapper from konami's site.",
  "exports": "./dist/index.js",
  "type": "module",
  "keywords": [
    "yu-gi-oh",
    "duel links"
  ],
  "scripts": {
    "dev-boxset": "tsx dev/boxSets.ts",
    "dev-boxeslist": "tsx dev/boxesList.ts",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "update-db": "node ./dist/core/script.js",
    "prettier-format": "prettier --cache -w .",
    "lint": "eslint . --cache --cache-location \"./node_modules/.cache/eslint/\"",
    "pre-commit": "pnpm prettier-format && pnpm lint"
  },
  "files": [
    "dist"
  ],
  "author": "Rodrigo Toledo",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.1",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "puppeteer": "^19.4.1"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falsepopsky/ygodl-box-scrapper.git"
  },
  "bugs": {
    "url": "https://github.com/falsepopsky/ygodl-box-scrapper/issues"
  }
}
