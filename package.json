{
  "name": "ygodl-box-scrapper",
  "version": "0.0.1",
  "description": "Web Scraper on Duel links's official site. Collecting the information contained from a selected box, like name, type and image cover.",
  "exports": "./dist/index.js",
  "type": "module",
  "keywords": [
    "puppeteer",
    "typescript",
    "yu-gi-oh",
    "duel links"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "start": "node dist/index.js",
    "prettier-format": "prettier --cache -w .",
    "lint": "eslint . --cache --cache-location \"./node_modules/.cache/eslint/\"",
    "pre-commit": "pnpm prettier-format && pnpm lint"
  },
  "files": [
    "dist"
  ],
  "author": "Rodrigo Toledo",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.2",
    "prettier": "^2.7.1",
    "tsx": "^3.12.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "puppeteer": "^19.2.2"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falsepopsky/ygodl-box-scrapper.git"
  },
  "bugs": {
    "url": "https://github.com/falsepopsky/ygodl-box-scrapper/issues"
  }
}
